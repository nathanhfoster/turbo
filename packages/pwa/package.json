{
  "name": "@nathanhfoster/pwa",
  "version": "0.0.0",
  "private": true,
  "description": "Progressive Web App utilities for React applications",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./providers": {
      "types": "./src/providers/InstallPromptProvider.tsx",
      "default": "./src/providers/InstallPromptProvider.tsx"
    },
    "./hooks": {
      "types": "./src/hooks/index.ts",
      "default": "./src/hooks/index.ts"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "default": "./src/types/index.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "default": "./src/utils/index.ts"
    },
    "./service-worker": {
      "types": "./src/utils/service-worker.ts",
      "default": "./src/utils/service-worker.ts"
    },
    "./offline-html": {
      "types": "./src/utils/offline-html.ts",
      "default": "./src/utils/offline-html.ts"
    },
    "./manifest-helpers": {
      "types": "./src/utils/manifest-helpers.ts",
      "default": "./src/utils/manifest-helpers.ts"
    },
    "./robots-helpers": {
      "types": "./src/utils/robots-helpers.ts",
      "default": "./src/utils/robots-helpers.ts"
    },
    "./settings": {
      "types": "./src/settings/index.tsx",
      "default": "./src/settings/index.tsx"
    },
    "./settings/page": {
      "types": "./src/settings/page.tsx",
      "default": "./src/settings/page.tsx"
    },
    "./device": {
      "types": "./src/providers/DeviceContextProvider/index.tsx",
      "default": "./src/providers/DeviceContextProvider/index.tsx"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@nathanhfoster/cookies": "workspace:*",
    "@nathanhfoster/resurrection": "workspace:*",
    "@nathanhfoster/ui": "workspace:*"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "next": "^14.0.0"
  },
  "devDependencies": {
    "@nathanhfoster/eslint-config": "workspace:*",
    "@nathanhfoster/typescript-config": "workspace:*",
    "@types/node": "^22.0.0",
    "@types/react": "19.2.3",
    "@types/react-dom": "19.2.3",
    "eslint": "^9.34.0",
    "typescript": "5.9.2"
  },
  "keywords": [
    "pwa",
    "progressive-web-app",
    "service-worker",
    "push-notifications",
    "install-prompt",
    "react",
    "nextjs"
  ]
}
