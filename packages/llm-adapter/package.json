{
  "name": "@nathanhfoster/llm-adapter",
  "version": "0.0.0",
  "private": true,
  "description": "Generic LLM provider adapter following SOLID principles. Supports multiple providers (OpenAI, Anthropic, Google, etc.) through a unified interface.",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./prompts/blogs": {
      "types": "./src/prompts/blogs/index.ts",
      "default": "./src/prompts/blogs/index.ts"
    }
  },
  "scripts": {
    "build": "echo 'No build output - type checking only'",
    "lint": "eslint .",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@nathanhfoster/utils": "workspace:*",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "@nathanhfoster/eslint-config": "workspace:*",
    "@nathanhfoster/typescript-config": "workspace:*",
    "@types/node": "^22.0.0",
    "eslint": "^9.34.0",
    "typescript": "5.9.2"
  },
  "keywords": [
    "llm",
    "ai",
    "adapter",
    "openai",
    "anthropic",
    "google",
    "provider",
    "chatgpt",
    "gpt",
    "solid"
  ]
}
